<Patch>

<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Royalty</li>
	</mods>
	<match Class="PatchOperationAdd">
		<xpath>/Defs/HediffDef[defName="MLRP_HarmonyChipInstalled"]/stages/li/statOffsets</xpath>
		<value>
			<ConversionPower>0.25</ConversionPower>
			<SuppressionPower>0.25</SuppressionPower>
			<SlaveSuppressionOffset>0.1</SlaveSuppressionOffset>
			<PruningSpeed>1</PruningSpeed>
			<HackingSpeed>0.25</HackingSpeed>
		</value>
	</match>
</Operation>

</Patch>
<Defs>

	<XmlExtensions.SettingsMenuDef>
		<modId>geodesicdragon.rimpony</modId>
		<defName>MLRP_XMLE_Settings</defName>
		<label>My Little RimPony</label>
		<defaultSpacing>0</defaultSpacing>
		<settings>
			<li Class="XmlExtensions.Setting.Text">
				<text>IMPORTANT: You will need to restart RimWorld after changing any of these settings.</text>
				<font>Medium</font>
				<anchor>Middle</anchor>
			</li>

			<li Class="XmlExtensions.Setting.Gap"/>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>Click here to view the My Little RimPony Wiki on GitHub.</text>
				<font>Small</font>
				<anchor>Middle</anchor>
				<actions>
				<li Class="XmlExtensions.Action.OpenURL">
					<url>https://github.com/GeodesicDragon/mylittlerimpony/wiki</url>
					</li>
				</actions>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap"/>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>Plushie Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Text" MayRequire="Ludeon.RimWorld.Biotech">
				<text>These settings will also be applied to child-sized plushies where appropriate.</text>
				<font>Small</font>
				<anchor>Right</anchor>
			</li>	
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_PlushiesCloth</key>
				<defaultValue>300</defaultValue>
				<min>2</min>
				<max>500</max>
				<label>Amount Of Stuff Needed: {MLRP_PlushiesCloth}</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Text" MayRequire="Ludeon.RimWorld.Biotech">
				<text>This value will be halved for child-sized plushies.</text>
				<font>Small</font>
				<anchor>Left</anchor>
			</li>

			<li Class="XmlExtensions.Setting.Gap"/>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_PlushieCraftingSkill</key>
				<defaultValue>5</defaultValue>
				<min>1</min>
				<max>20</max>
				<label>Crafting Skill Required: {MLRP_PlushieCraftingSkill}</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox">
				<key>MLRP_PlushiesTaintedOnDeath</key>
				<label>Plushies become tainted if worn by a corpse</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox">
				<key>MLRP_UseOldTwilightTexture</key>
				<label>Use old Twilight Sparkle texture</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_PlushieRecyclingStuffGain</key>
				<defaultValue>75</defaultValue>
				<min>50</min>
				<max>250</max>
				<label>Recycling plushies will recover {MLRP_PlushieRecyclingStuffGain} materials</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox" MayRequire="Dubwise.DubsBadHygiene">
				<key>MLRP_DubsBadHygiene_PippIsShort</key>
				<label>[Dubs Bad Hygiene] Pipp is short</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap"/>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>Faction Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox">
				<key>MLRP_NPCFactionIsPresentInNewGame</key>
				<label>NPC faction is always present when starting a new colony</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox" MayRequire="Ludeon.RimWorld.Biotech">
				<key>MLRP_DiamondDogFactionIsPresentInNewGame</key>
				<label>Diamond Dog faction is always present when starting a new colony</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox" MayRequire="sarg.alphagenes">
				<key>MLRP_BatponyFactionIsPresentInNewGame</key>
				<label>Batpony faction is always present when starting a new colony</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap"/>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>Rainbow Laser Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox">
				<key>MLRP_RainbowLaserRequiresEOH</key>
				<label>Weapon requires Elements of Harmony to craft</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Text" MayRequire="Ludeon.RimWorld.Biotech">
				<text>If you set this value to false, making a rainbow laser will require 3 advanced components instead of the Elements.</text>
				<font>Small</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap"/>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>Evil Sisters Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_EvilSistersStuffRequired</key>
				<defaultValue>750</defaultValue>
				<min>100</min>
				<max>1000</max>
				<label>Building either sister requires {MLRP_EvilSistersStuffRequired} stuff</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap"/>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>Discord Lamp Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_DiscordLampStuffRequired</key>
				<defaultValue>300</defaultValue>
				<min>100</min>
				<max>1000</max>
				<label>Building a Discord lamp requires {MLRP_DiscordLampStuffRequired} stuff</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_DiscordLampBrightness</key>
				<defaultValue>8</defaultValue>
				<min>1</min>
				<max>20</max>
				<label>Brightess: {MLRP_DiscordLampBrightness}</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap"/>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>Screwball Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_ScrewballStuffRequired</key>
				<defaultValue>300</defaultValue>
				<min>100</min>
				<max>1000</max>
				<label>Building Screwballs requires {MLRP_ScrewballStuffRequired} stuff</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap"/>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>Tree Hugger Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_TreeHuggerStuffRequired</key>
				<defaultValue>200</defaultValue>
				<min>100</min>
				<max>1000</max>
				<label>Building Tree Hugger requires {MLRP_TreeHuggerStuffRequired} stuff</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap"/>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>Sweetie Bot Turret Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_SweetieBotWeaponRange</key>
				<defaultValue>46</defaultValue>
				<min>10</min>
				<max>100</max>
				<label>Attack Range: {MLRP_SweetieBotWeaponRange}</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_SweetieBotAccuracyTouch</key>
				<defaultValue>0.3</defaultValue>
				<min>0.1</min>
				<max>1</max>
				<label>Touch Accuracy: {MLRP_SweetieBotAccuracyTouch}</label>
				<decimals>1</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_SweetieBotAccuracyShort</key>
				<defaultValue>0.4</defaultValue>
				<min>0.1</min>
				<max>1</max>
				<label>Short Range Accuracy: {MLRP_SweetieBotAccuracyShort}</label>
				<decimals>1</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_SweetieBotAccuracyMedium</key>
				<defaultValue>0.8</defaultValue>
				<min>0.1</min>
				<max>1</max>
				<label>Medium Range Accuracy: {MLRP_SweetieBotAccuracyMedium}</label>
				<decimals>1</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_SweetieBotAccuracyLong</key>
				<defaultValue>0.8</defaultValue>
				<min>0.1</min>
				<max>1</max>
				<label>Long Range Accuracy: {MLRP_SweetieBotAccuracyLong}</label>
				<decimals>1</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap"/>

			<li Class="XmlExtensions.Setting.Text">
				<text>Trait Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_BronyTraitCommonality</key>
				<defaultValue>0.1</defaultValue>
				<min>0</min>
				<max>1</max>
				<label>Brony Trait Commonality: {MLRP_BronyTraitCommonality}</label>
				<decimals>1</decimals>
			</li>
					
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_AntiBronyTraitCommonality</key>
				<defaultValue>0.2</defaultValue>
				<min>0</min>
				<max>1</max>
				<label>Anti Brony Trait Commonality: {MLRP_AntiBronyTraitCommonality}</label>
				<decimals>1</decimals>
			</li>
						
			<li Class="XmlExtensions.Setting.Gap"/>

			<li Class="XmlExtensions.Setting.Text">
				<text>Nurse Redheart Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox">
				<key>MLRP_NurseRedheartSpawnGlitterworldMedicine</key>
				<label>Spawn glitterworld medicine</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Text" MayRequire="geodesicdragon.rimpony.medieval">
				<text>IMPORTANT: This setting will be ignored because you are using the Medieval Overhaul patches.</text>
				<font>Small</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_NurseRedheartMedicineSpawnAmount</key>
				<defaultValue>5</defaultValue>
				<min>1</min>
				<max>30</max>
				<label>Amount of medicine to spawn: {MLRP_NurseRedheartMedicineSpawnAmount}</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_NurseRedheartMedicineSpawnTime</key>
				<defaultValue>10</defaultValue>
				<min>1</min>
				<max>90</max>
				<label>Medicine Spawn Time (days): {MLRP_NurseRedheartMedicineSpawnTime}</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap"/>

			<li Class="XmlExtensions.Setting.Text">
				<text>Aggro Turret Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
						
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_ZephyrBreezeHealth</key>
				<defaultValue>1000</defaultValue>
				<min>100</min>
				<max>3000</max>
				<label>Health: {MLRP_ZephyrBreezeHealth}</label>
				<decimals>0</decimals>
			</li>
			
			
			<li Class="XmlExtensions.Setting.Gap"/>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>Super Speedy Cider Squeezy 6000 Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
							
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_SSCS6KBeerSpawnAmount</key>
				<defaultValue>30</defaultValue>
				<min>1</min>
				<max>100</max>
				<label>Amount of beer to spawn: {MLRP_SSCS6KBeerSpawnAmount}</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_SSCS6KBeerSpawnTime</key>
				<defaultValue>30</defaultValue>
				<min>1</min>
				<max>90</max>
				<label>Beer Spawn Time (days): {MLRP_SSCS6KBeerSpawnTime}</label>
				<decimals>0</decimals>
			</li>

			<li Class="XmlExtensions.Setting.Gap"/>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>Daybreaker Statue Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_DaybreakerChemfuel</key>
				<defaultValue>50</defaultValue>
				<min>2</min>
				<max>150</max>
				<label>Amount of chemfuel required: {MLRP_DaybreakerChemfuel}</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>This amount will be halved, doubled, tripled or quintupled where necessary.</text>
				<font>Small</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Text" MayRequire="Ryflamer.Rimcraft.Metals">
				<text>[Ry]Rimcraft Metals: Recipes to bulk create bars from ore will use triple this amount.</text>
				<font>Small</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Text" MayRequire="DankPyon.Medieval.Overhaul">
				<text>Medieval Overhaul: These values do not apply to coal usage.</text>
				<font>Small</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap"/>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>Derpy Hooves Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider">
				<key>MLRP_DerpyHoovesMassCapacity</key>
				<defaultValue>300</defaultValue>
				<min>300</min>
				<max>1200</max>
				<label>Mass Capacity: {MLRP_DerpyHoovesMassCapacity}</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>This will also determine how much stuff is required to build one.</text>
				<font>Small</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap" MayRequire="Ludeon.RimWorld.Royalty"/>
			
			<li Class="XmlExtensions.Setting.Text" MayRequire="Ludeon.RimWorld.Royalty">
				<text>Royalty DLC Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox" MayRequire="Ludeon.RimWorld.Royalty">
				<key>MLRP_TurnAnimaTreeIntoTreeOfHarmony</key>
				<label>Turn the anima tree into the Tree of Harmony</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap" MayRequire="Ludeon.RimWorld.Ideology"/>
			
			<li Class="XmlExtensions.Setting.Text" MayRequire="Ludeon.RimWorld.Ideology">
				<text>Ideology DLC Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox" MayRequire="Ludeon.RimWorld.Ideology">
				<key>MLRP_HarmonyPreceptProvidesIncreasedBonuses</key>
				<label>Harmony precept provides increased bonuses</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap" MayRequire="Ludeon.RimWorld.Biotech"/>
			
			<li Class="XmlExtensions.Setting.Text" MayRequire="Ludeon.RimWorld.Biotech">
				<text>Biotech DLC Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox" MayRequire="Ludeon.RimWorld.Biotech">
				<key>MLRP_RainbowDashChildPlushUsesAdultStats</key>
				<label>Child-equippable Rainbow Dash plushie has the same bonuses as the adult version</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox" MayRequire="Ludeon.RimWorld.Biotech">
				<key>MLRP_SweetieBotMechanoidsAppearInMechClusters</key>
				<label>Sweetie Bot mechanoids appear in mechanoid clusters</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox" MayRequire="Ludeon.RimWorld.Biotech">
				<key>MLRP_SweetieBotMechanoidsAreQuiet</key>
				<label>Mute Sweetie Bot mechanoid call sound</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap" MayRequire="Ludeon.RimWorld.Anomaly"/>
			
			<li Class="XmlExtensions.Setting.Text" MayRequire="Ludeon.RimWorld.Anomaly">
				<text>Anomaly DLC Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Slider" MayRequire="Ludeon.RimWorld.Anomaly">
				<key>MLRP_ThingponeStuffRequired</key>
				<defaultValue>250</defaultValue>
				<min>100</min>
				<max>750</max>
				<label>Building Thingpone requires {MLRP_ThingponeStuffRequired} stuff</label>
				<decimals>0</decimals>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap" MayRequire="Ludeon.RimWorld.Odyssey"/>
			
			<li Class="XmlExtensions.Setting.Text" MayRequire="Ludeon.RimWorld.Odyssey">
				<text>Odyssey DLC Settings</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Text">
				<text>Nothing yet.</text>
				<font>Small</font>
				<anchor>Left</anchor>
			</li>
									
			<li Class="XmlExtensions.Setting.Gap" MayRequire="sumghai.medpod"/>
			
			<li Class="XmlExtensions.Setting.Text" MayRequire="sumghai.medpod">
				<text>Mod Settings: MedPod</text>
				<font>Medium</font>
				<anchor>Left</anchor>
			</li>
			
			<li Class="XmlExtensions.Setting.Checkbox" MayRequire="sumghai.medpod">
				<key>MLRP_MedPodsDoNotCureCutiePox</key>
				<label>Prevent MedPods from curing the Cutie Pox</label>
			</li>
			
			<li Class="XmlExtensions.Setting.Gap">
				<spacing>8</spacing>
			</li>
									
			<li Class="XmlExtensions.Setting.GapLine"/>
			
			<li Class="XmlExtensions.Setting.ResetSettings"/>
			
			<li Class="XmlExtensions.Setting.Gap">
				<spacing>8</spacing>
			</li>
			
			<li Class="XmlExtensions.Setting.ApplyActions">
				<label>Restart Game</label>
				<actions>
					<li Class="XmlExtensions.Action.RestartGame"/>
				</actions>
			</li>
		</settings>
	</XmlExtensions.SettingsMenuDef>

</Defs>
<Defs>

<!--SHOOTING-->

<RecipeDef>
	<defName>MLRP_NMM_ShootingSkilltrainer</defName>
	<label>request skilltrainer (shooting)</label>
	<description>Offer chocolate in exchange for a skilltrainer (shooting).</description>
	<researchPrerequisite>MLRP_GetSkilltrainersFromNMM</researchPrerequisite>
    <jobString>Making an offering to Nightmare Moon</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <workSkill>Crafting</workSkill>
    <effectWorking>Cremate</effectWorking>
    <soundWorking>Recipe_Cremate</soundWorking>
    <allowMixingIngredients>false</allowMixingIngredients>
    <workAmount>300</workAmount>
	<recipeUsers>
		<li>MLRP_NightmareMoon</li>
	</recipeUsers>
    <ingredients>
		<li>
			<filter>
				<thingDefs>
					<li>Chocolate</li>
				</thingDefs>
			</filter>
			<count>1125</count>
		</li>
	</ingredients>
	<products>
		<Neurotrainer_Shooting>1</Neurotrainer_Shooting>
	</products>
</RecipeDef>


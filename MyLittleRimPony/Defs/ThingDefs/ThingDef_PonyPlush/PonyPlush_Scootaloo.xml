<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <ThingDef ParentName="PonyPlushBase">
    <thingClass>Apparel</thingClass>
    <defName>PonyPlush_Scootaloo</defName>
    <label>Scootaloo</label>
    <description>Scootaloo has had plenty of experience riding her scooter, but she's also had plenty of accidents.\n\nCarrying her makes you feel as though your injuries are healing faster.</description>
    <techLevel>Industrial</techLevel>
    <graphicData>
      <texPath>Things/PonyPlush/Scootaloo</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1</drawSize>
    </graphicData>
    <equippedStatOffsets>
      <InjuryHealingFactor>0.5</InjuryHealingFactor>
    </equippedStatOffsets>
    <apparel>
  <careIfWornByCorpse>true</careIfWornByCorpse>
  <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <wornGraphicPath>Things/PonyPlush/PlushEquipped</wornGraphicPath>
      <layers>
        <li>Belt</li>
      </layers>
      <bodyPartGroups>
        <li>Waist</li>
      </bodyPartGroups>
    <useWornGraphicMask>true</useWornGraphicMask>
    </apparel>
  </ThingDef>
</Defs>
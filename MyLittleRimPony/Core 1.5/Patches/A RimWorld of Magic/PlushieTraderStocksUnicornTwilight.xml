<Patch>

<Operation Class="PatchOperationFindMod">
    <mods>
        <li>A RimWorld of Magic</li>
    </mods>
	<match Class="PatchOperationAdd">
		<xpath>/Defs/TraderKindDef[defName="MLRP_PlushieTrader"]/stockGenerators</xpath>
		<value>
			<li Class="StockGenerator_SingleDef">
				<thingDef>PonyPlush_UnicornTwilight</thingDef>
				<countRange>1</countRange>
			</li>
		</value>
	</match>
</Operation>

</Patch>